<body>
	<% include partials/navbar %>
	<div class="container">
		<div class="jumbotron">
			<div class="container">
				<h3>Reports</h3>
				<table>
					<tbody>
						<tr>
							<th>Student</th>
							<th>Teacher</th>
							<th>Score</th>
							<th>Lab</th>
							<th>Time</th>
							<th>Date</th>
						</tr>
	            			<% for (var i in results) { %>
			            <tr>
			                <td>
			                        <%= results[i].studentName %>
			                    </td>
			                    <td>
			                        <%= results[i].teacherName %>
			                    </td>
			                    <td>
			                        <%= results[i].score %>
			                    </td>
			                    <td>
			                        <%= results[i].labName %>
			                    </td>
			                    <td>
			                        <%= results[i].time %>
			                    </td>
			                    <td>
			                        <%= results[i].date %>
			                    </td>
			                    
			                <% } %>
			            </tr>
        			</tbody>
    			</table>
			</div>
		</div>
	</div>
